{
  "name": "insidex",
  "version": "1.0.0",
  "description": "InsideX storefront",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "test": "node node_modules/prisma/build/index.js generate && NODE_OPTIONS=--experimental-vm-modules node ./node_modules/jest/bin/jest.js",
    "test:e2e": "node node_modules/prisma/build/index.js generate && NODE_OPTIONS=--experimental-vm-modules node ./node_modules/jest/bin/jest.js tests/admin-ui-visibility.e2e.test.js",
    "test:coverage": "node node_modules/prisma/build/index.js generate && NODE_OPTIONS=--experimental-vm-modules node ./node_modules/jest/bin/jest.js --coverage",
    "test:coverage:jest": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --runInBand --coverage",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "node prisma/seed.js",
    "db:import": "node scripts/import-json-to-db.js",
    "db:verify:integration": "node scripts/verify-db-integration.js",
    "epic:1.4:verify": "node scripts/verify-epic-1.4.js",
    "scan:floats": "node scripts/scan-float-usage.js",
    "test:coverage:ci": "node scripts/ci/run-coverage-ci.js",
    "test:chaos": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --runInBand --detectOpenHandles --testPathPattern=\"tests[\\/]+chaos[\\/]+.*\\.test\\.js\""
  },
  "dependencies": {
    "@prisma/client": "^6.0.0",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "caniuse-lite": "^1.0.30001770",
    "cross-env": "^10.1.0",
    "jest": "^29.7.0",
    "prisma": "^6.0.0",
    "supertest": "^7.0.0"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}