{
  "name": "insidex",
  "version": "1.0.0",
  "description": "InsideX storefront",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "test": "node node_modules/prisma/build/index.js generate && NODE_OPTIONS=--experimental-vm-modules node ./node_modules/jest/bin/jest.js --runInBand",
    "test:e2e": "node node_modules/prisma/build/index.js generate && NODE_OPTIONS=--experimental-vm-modules node ./node_modules/jest/bin/jest.js --runInBand tests/admin-ui-visibility.e2e.test.js",
    "test:coverage": "node node_modules/prisma/build/index.js generate && NODE_OPTIONS=--experimental-vm-modules node ./node_modules/jest/bin/jest.js --runInBand --coverage",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "node prisma/seed.js",
    "db:import": "node scripts/import-json-to-db.js",
    "db:verify:integration": "node scripts/verify-db-integration.js",
    "epic:1.4:verify": "node scripts/verify-epic-1.4.js"
  },
  "dependencies": {
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.23.8",
    "@prisma/client": "^6.0.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "prisma": "^6.0.0"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}